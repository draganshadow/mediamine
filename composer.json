        "phploc/phploc": "*",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "2.0.*@dev",
        "pdepend/pdepend" : "dev-master",
        "ruflin/elastica": "0.20.5.*@dev",
        "zendframework/zend-developer-tools": "dev-master",
        "widmogrod/zf2-assetic-module": "1.*",
        "zircote/swagger-php": "0.9.*",
