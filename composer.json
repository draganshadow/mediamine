{
    "name": "mediamine",
    "description": "Multimedia library",
    "license": "Private",
    "keywords": ["zf2", "doctrine", "video", "music"],
    "homepage": "https://github.com/draganshadow/mediamine",

    "authors": [
        {
            "name": "draganshadow",
            "email": "draganshdow@netsyos.com"
        }
